	s.pageName="avalon|"+analyticsPageName;
} else if (typeof(Drupal) != 'undefined') {
	s.pageName="avalon|"+Drupal.settings.omniturePageName;
} else {
	s.pageName="avalon|other|"+window.location.pathname;
}

// replace trailing slash US9487:
